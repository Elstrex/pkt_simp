/!FUNC Fib(n) ->
    IF (n < 0) THEN
        0
    ELSE 
        IF (n == 1) THEN
            0
        ELSE
            IF (n == 2) THEN
                1
            ELSE 
                Fib(n - 1) + Fib(n - 2)
            END
        END
    END
END;

